{{#hvisTekstFinnes (concat tekst ".sporsmal")}}
    {{#hvisTekstFinnes (concat tekst ".undertittel")}}
        <h{{{niva}}}>{{{ hentTekst (concat tekst ".undertittel") }}}</h{{{niva}}}>
        {{#variabel "niva" (inc niva)}}
            <h{{niva}}>{{{ hentTekst (concat tekst ".sporsmal") }}}</h{{niva}}>
        {{/variabel}}
    {{else}}
        <h{{niva}}>{{{ hentTekst (concat tekst ".sporsmal") }}}</h{{niva}}>
    {{/hvisTekstFinnes}}
{{/hvisTekstFinnes}}