<h2>{{ hentTekst "andreytelser.tittel" }}</h2>

<h3>{{ hentTekst "andreytelser.ytelserfraNAV.overskrift" }}</h3>
<table>
    {{#forFaktum "andreytelser.ytelserfraNAV.kvalifiseringsstonad"}}
        {{#hvisSant value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "andreytelser.ytelserfraNAV.kvalifiseringsstonad"}}</span>
                </td>
            </tr>
        {{/hvisSant}}
    {{/forFaktum}}
    {{#forFaktum "andreytelser.ytelserfraNAV.okonomisksosialhjelp"}}
        {{#hvisSant value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "andreytelser.ytelserfraNAV.okonomisksosialhjelp"}}</span>
                </td>
            </tr>
        {{/hvisSant}}
    {{/forFaktum}}
    {{#forFaktum "andreytelser.ytelserfraNAV.ikkemottattellersokt"}}
        {{#hvisSant value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "andreytelser.ytelserfraNAV.ikkemottattellersokt"}}</span>
                </td>
