        {{/hvisSant}}
    {{/forFaktum}}
</table>

{{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver"}}
    {{#hvisIkkeTom value}}
        <h3>{{ hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.overskrift" }}</h3>
        <table>
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisSant value}}
                        <span>{{ hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.true" }}</span>
                    {{ else }}
                        <span>{{ hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.false" }}</span>
                        <ul>
                            {{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver.utbetalingen.engangsbelop"}}
                                {{#hvisSant value}}
                                    <li class="kulepunkt">
                                        <span>{{ hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.utbetalingen.engangsbelop" }}. </span>
                                        {{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver.engangsbelop.belop"}}
                                            {{#hvisIkkeTom value}}
                                                <span>{{hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.utbetalingen.engangsbelop.belop.label"}}: {{ value }} kr</span>
                                            {{/hvisIkkeTom}}
                                        {{/forFaktum}}
                                    </li>
                                {{/hvisSant}}
                            {{/forFaktum}}
                            {{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver.utbetalingen.lopendeutbetaling"}}
                                {{#hvisSant value}}
                                    <li class="kulepunkt">
                                        <span>{{ hentTekst "andreytelser.utbetalinger.fraarbeidsgiver.utbetalingen.lopendeutbetaling" }}</span>
                                        <ul>
                                            {{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver.lopendeutbetaling.belop"}}
                                                {{#hvisIkkeTom value}}
                                                    <li class="kulepunkt">
                                                        <span>{{hentTekst "oppsummering.andreYtelser.beloepPerMaaned"}}: {{ value }} kr</span>
                                                    </li>
                                                {{/hvisIkkeTom}}
                                            {{/forFaktum}}
                                            {{#forFaktum "andreytelser.utbetalinger.fraarbeidsgiver.lopendeutbetaling.opphorer"}}
                                                {{#hvisIkkeTom properties}}
                                                    <li class="kulepunkt">
                                                        <span>{{hentTekst "oppsummering.andreYtelser.opphoerer"}}: {{ toLowerCase properties.maaned }} {{ properties.aar }}</span>
                                                    </li>
                                                {{/hvisIkkeTom}}
                                            {{/forFaktum}}
                                        </ul>
                                    </li>
                                {{/hvisSant}}
                            {{/forFaktum}}
                        </ul>
                    {{/hvisSant}}
                </td>
            </tr>
        </table>
    {{/hvisIkkeTom}}
{{/forFaktum}}


