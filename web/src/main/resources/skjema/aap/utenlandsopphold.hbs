{{#forFaktum "utenlandsopphold.oppholdINorgeSisteTreAar"}}
    {{#hvisIkkeTom value}}
        <h2>{{ hentTekst "oppsummering.utenlandsopphold.tittel" }}</h2>
        <h3>{{ hentTekst "utenlandsopphold.oppholdINorgeSisteTreAar.overskrift" }}</h3>
        <table>
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisSant value}}
                        <span>{{ hentTekst "aap.ordinaer.utenlandsopphold.oppholdINorgeSisteTreAar.true" }}</span>
                    {{else}}
                        <span>{{ hentTekst "aap.ordinaer.utenlandsopphold.oppholdINorgeSisteTreAar.false" }}</span>

                        {{#forFaktum "utenlandsopphold.oppholdINorgeSisteTreAar.false.boddINorgeSisteAaret"}}
                            {{#hvisSant value}}
                                <li>
                                    <span>{{ hentTekst "aap.ordinaer.utenlandsopphold.oppholdINorgeSisteTreAar.false.boddINorgeSisteAaret"}}</span>
                                </li>
                            {{/hvisSant}}
                        {{/forFaktum}}
                        {{#forFaktum "utenlandsopphold.oppholdINorgeSisteTreAar.false.boddIEOSLandSisteTreAar"}}
                            {{#hvisSant value}}
                                <li>
                                    <span>{{ hentTekst "utenlandsopphold.oppholdINorgeSisteTreAar.false.boddIEOSLandSisteTreAar"}}</span>
                                </li>
                            {{/hvisSant}}
                        {{/forFaktum}}
                        {{#forFaktum "utenlandsopphold.oppholdINorgeSisteTreAar.false.registrertFlyktning"}}
                            {{#hvisSant value}}
                                <li>
                                    <span>{{ hentTekst "utenlandsopphold.oppholdINorgeSisteTreAar.false.registrertFlyktning"}}</span>
                                </li>
                            {{/hvisSant}}
                        {{/forFaktum}}
                    {{/hvisSant}}
                </td>
            </tr>
        </table>
    {{/hvisIkkeTom}}
{{/forFaktum}}
{{#forFaktum "utenlandsopphold.EOSLand"}}
    {{#hvisIkkeTom value}}
        <h3>{{ hentTekst "utenlandsopphold.EOSLand.overskrift" }}</h3>
        <table>
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisSant value}}
                        <span>{{ hentTekst "utenlandsopphold.EOSLand.true" }}</span>
                    {{else}}
                        <span>{{ hentTekst "utenlandsopphold.EOSLand.false" }}</span>

                        {{#forFakta "utenlandsopphold.EOSLand.periode"}}
                            <li>
                                <span>Land: {{ hentLand properties.eosLand }},</span>
                                <span>fra {{ toLowerCase properties.fraMaaned }} {{ properties.fraAar }}</span>
                                <span>til {{ toLowerCase properties.tilMaaned }} {{ properties.tilAar }}</span>
                            </li>
                        {{/forFakta}}
                    {{/hvisSant}}
                </td>
            </tr>
        </table>
    {{/hvisIkkeTom}}
{{/forFaktum}}