{{#forFaktum "yrkesskade.soeknadsgrunn"}}
    {{#hvisIkkeTom value}}
        <h2>{{ hentTekst "yrkesskade.tittel" }}</h2>
        <table>
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisLik value "yrkesskadeGodkjent"}}
                        <span>{{ hentTekst "yrkesskade.soeknadsgrunn.yrkesskade.godkjent" }}</span>
                    {{/hvisLik}}
                    {{#hvisLik value "yrkesskadeIkkeSvarPaaSoeknad"}}
                        <span>{{ hentTekst "yrkesskade.soeknadsgrunn.yrkesskade.ikkeSvarPaaSoeknad" }}</span>
                    {{/hvisLik}}
                    {{#hvisLik value "yrkesskadeAnnet"}}
                        <span>{{ hentTekst "yrkesskade.soeknadsgrunn.annet" }}</span>
                    {{/hvisLik}}
                </td>
            </tr>
        </table>
    {{/hvisIkkeTom}}
{{/forFaktum}}