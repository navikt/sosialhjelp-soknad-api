<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <base href="/sendsoknad/"></base>
    {{> skjema/generisk/generisk-styling }}
</head>

<body>

<div class="header">
    <img class="header-logo" src="img/nav-logo.gif"/>
    {{#with data.personalia}}
        <span class="navn">{{navn.fornavn}} {{navn.mellomnavn}} {{navn.etternavn}}</span>
        <span class="fnr">{{personIdentifikator.verdi}}</span>
    {{/with}}
    <span class="headerSider">
        {{ hentTekst "oppsummering.sideAvTotal" }}
    </span>
</div>

<div class="footer">
    <span>
        <span>NAV 35-18.01 {{ hentTekst "oppsummering.eSoknad" }}</span>
    </span>
</div>

<div id="soknad" class="generisk-oppsummering">

    <div class="hode">
        <h1>Søknad om økonomisk sosialhjelp</h1>
        <div class="personalia">
            {{#with data.personalia}}
                <p class ="nvn">{{navn.fornavn}} {{navn.mellomnavn}} {{navn.etternavn}}</p>
                <p class ="fnr">{{personIdentifikator.verdi}}</p>
                {{#with folkeregistrertAdresse}}
                    <p class ="adr">{{gatenavn}} {{husnummer}}{{husbokstav}}, {{postnummer}} {{poststed}}</p>
                {{/with}}
                {{#if kontonummer.verdi}}
                <div>
                    <span>Kontonummer</span>:
                    <span>{{kontonummer.verdi}}</span>
                </div>
                {{/if}}
            {{/with}}
        </div>
    </div>
    <div>
    	{{> skjema/ny_generisk/personalia}}
    </div>
</div>
<div class="js-signatur"></div>

</body>
</html>