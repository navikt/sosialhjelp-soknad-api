            <td class="sjekket"></td>
            <td>
                <span>{{ hentTekst "soknad.laeremidler.funksjonshemming.belop.oppsummering" }} {{properties.utgift}}  {{hentTekst "soknad.kr"}} </span>
            </td>
        </tr>
    {{/hvisSant}}
