{{#forFaktum "informasjonsside.stonad.barnepass"}}
    {{#hvisSant value}}
        <h2>{{hentTekst "soknad.barnepass.tittel"}}</h2>
        <table>
        {{#forFaktum "barnepass.periode"}}
            {{> skjema/tilleggsstonader/felles/aktivitetsperiode}}
        {{/forFaktum}}
        {{#forFakta "barn"}}
            {{> skjema/tilleggsstonader/barnepass/barnepassForBarn}}
        {{/forFakta}}
        {{#forFaktum "barnepass.utbetalingsdato"}}
            {{> skjema/tilleggsstonader/felles/utbetalingsdato}}
        {{/forFaktum}}
        </table>
    {{/hvisSant}}
{{/forFaktum}}
