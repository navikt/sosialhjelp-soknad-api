{{#forFaktum "behovforbil.behovforegenbil"}}
    {{#hvisLik value "ja"}}
        <tr>
            <td class="sjekket"></td>
            <td>{{hentTekst "arbeidutdanning.behovforbil.behovforegenbil.ja"}}</td>
        </tr>

        {{#forFaktumHvisSant "behovforbil.stotteordninger.harttkort"}}
            <tr>
                <td class="sjekket"></td>
                <td>{{ hentTekst "arbeidutdanning.behovforbil.stotteordninger.harttkort"}}</td>
            <tr>
        {{/forFaktumHvisSant}}

        {{#forFaktumHvisSant "behovforbil.stotteordninger.grunnstonad"}}
            <tr>
                <td class="sjekket"></td>
                <td>{{ hentTekst "arbeidutdanning.behovforbil.stotteordninger.grunnstonad"}}</td>
            <tr>
        {{/forFaktumHvisSant}}

        {{#forFaktumHvisSant "behovforbil.stotteordninger.soktttkort"}}
            <tr>
                <td class="sjekket"></td>
                <td>{{ hentTekst "arbeidutdanning.behovforbil.stotteordninger.soktttkort"}}</td>
            <tr>
        {{/forFaktumHvisSant}}

        {{#forFaktum "behovforbil.antallreisertilfraarbeid"}}
            <tr>
                <td class="sjekket"></td>
                <td><span>{{ hentTekst "arbeidutdanning.behovforbil.antallreisertilfraarbeid" }}</span><br/><em>{{ value }}</em></td>
            </tr>

        {{/forFaktum}}

        {{#forFaktum "behovforbil.avstandtilarbeidsplass"}}
            <tr>
                <td class="sjekket"></td>
                <td><span>{{ hentTekst "arbeidutdanning.behovforbil.avstandtilarbeidsplass" }}</span><br/><em>{{ value }}</em> {{ hentTekst "enhet.kilometer"}}</td>
            </tr>

        {{/forFaktum}}

        {{#forFaktum "behovforbil.hvordandekkesbehov.beskrivelse"}}
            <tr>
                <td class="sjekket"></td>
                <td><span>{{ hentTekst "arbeidutdanning.behovforbil.hvordandekkesbehov.beskrivelse.sporsmal" }}</span><br/><em>{{ value }}</em></td>
            </tr>
        {{/forFaktum}}
    {{/hvisLik}}

    {{#hvisLik value "nei"}}
        <tr>
            <td class="sjekket"></td>
            <td>{{hentTekst "arbeidutdanning.behovforbil.behovforegenbil.nei"}}</td>
        </tr>
    {{/hvisLik}}
{{/forFaktum}}
