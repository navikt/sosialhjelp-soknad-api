<h2>{{ hentTekst "behovforbil.tittel" }}</h2>
<table>
    {{#forFaktum "behovforbil.reisealene"}}
        {{#hvisLik value "nei"}}
            <tr>
                <td class="sjekket"></td>
                <td>{{ hentTekst "behovforbil.reisealene.nei"}}</td>
            </tr>
            {{#forFaktum "behovforbil.reisemedfolge"}}
                {{#hvisLik value "ja"}}
                    <tr>
                        <td class="sjekket"></td>
                        <td>{{ hentTekst "behovforbil.reisemedfolge.ja" }}</td>
                    </tr>
                {{/hvisLik}}
                {{#hvisLik value "nei"}}
                    <tr>
                        <td class="sjekket"></td>
                        <td>{{ hentTekst "behovforbil.reisemedfolge.nei" }}</td>
                    </tr>
                {{/hvisLik}}
            {{/forFaktum}}
            {{#forFaktumHvisSant "behovforbil.belastende.avellerpakollektivtransport"}}
                <tr>
                    <td class="sjekket"></td>
                    <td>{{ hentTekst "behovforbil.belastende.avellerpakollektivtransport.label"}}</td>
                <tr>
            {{/forFaktumHvisSant}}
            {{#forFaktumHvisSant "behovforbil.belastende.ingenholdeplasser"}}
                <tr>
                    <td class="sjekket"></td>
                    <td>{{ hentTekst "behovforbil.belastende.ingenholdeplasser.label"}}</td>
                <tr>
            {{/forFaktumHvisSant}}
            {{#forFaktumHvisSant "behovforbil.belastende.langttilholdeplasser"}}
                <tr>
                    <td class="sjekket"></td>
                    <td>
                        <span>{{ hentTekst "behovforbil.belastende.langttilholdeplasser.label"}}</span>
                        <ul>
                            {{#forFaktum "behovforbil.belastende.langttilholdeplasser.buss"}}
                                <li class="kulepunkt faktum-felt">{{ hentTekst "behovforbil.belastende.langttilholdeplasser.buss.label" }}: {{value}} {{ hentTekst "behovforbil.belastende.langttilholdeplasser.enhet"}}</li>
                            {{/forFaktum}}
                            {{#forFaktum "behovforbil.belastende.langttilholdeplasser.trikk"}}
                                <li class="kulepunkt faktum-felt">{{ hentTekst "behovforbil.belastende.langttilholdeplasser.trikk.label" }}: {{value}} {{ hentTekst "behovforbil.belastende.langttilholdeplasser.enhet"}}</li>
                            {{/forFaktum}}
                            {{#forFaktum "behovforbil.belastende.langttilholdeplasser.bane"}}
                                <li class="kulepunkt faktum-felt">{{ hentTekst "behovforbil.belastende.langttilholdeplasser.bane.label" }}: {{value}} {{ hentTekst "behovforbil.belastende.langttilholdeplasser.enhet"}}</li>
                            {{/forFaktum}}
                            {{#forFaktum "behovforbil.belastende.langttilholdeplasser.tog"}}
                                <li class="kulepunkt faktum-felt">{{ hentTekst "behovforbil.belastende.langttilholdeplasser.tog.label" }}: {{value}} {{ hentTekst "behovforbil.belastende.langttilholdeplasser.enhet"}}</li>
                            {{/forFaktum}}
                            {{#forFaktum "behovforbil.belastende.langttilholdeplasser.bat"}}
                                <li class="kulepunkt faktum-felt">{{ hentTekst "behovforbil.belastende.langttilholdeplasser.bat.label" }}: {{value}} {{ hentTekst "behovforbil.belastende.langttilholdeplasser.enhet"}}</li>
                            {{/forFaktum}}
                        </ul>
                    </td>
                </tr>
            {{/forFaktumHvisSant}}
            {{#forFaktumHvisSant "behovforbil.belastende.andrearsaker"}}
                <tr>
                    <td class="sjekket"></td>
                    <td>
                        <p>{{ hentTekst "behovforbil.belastende.andrearsaker.label"}}</p>
                        {{#forFaktum "behovforbil.belastende.andrearsaker.beskrivelse"}}
                            <em>{{ value }}</em>
                        {{/forFaktum}}
                    </td>
                <tr>
            {{/forFaktumHvisSant}}
        {{/hvisLik}}
        {{#hvisLik value "ja"}}
            <tr>
                <td class="sjekket"></td>
                <td>{{ hentTekst "behovforbil.reisealene.ja"}}</td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}
</table>