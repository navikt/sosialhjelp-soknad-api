{{#forFaktum "fastlege.harIkkeFastlege"}}
    <h2>{{ hentTekst "fastlege.tittel" }}</h2>
    <table>
        {{#hvisSant value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <div>{{ hentTekst "oppsummering.fastlege.behandlende.navn" }}: {{ hentFaktumValue "fastlege.annen.behandlende.navn" }}</div>
                    <div>{{ hentTekst "oppsummering.fastlege.behandlende.stilling" }}: {{ hentFaktumValue "fastlege.annen.behandlende.stilling" }}</div>
                    <div>{{ hentTekst "oppsummering.fastlege.behandlende.arbeidssted" }}: {{ hentFaktumValue "fastlege.annen.behandlende.arbeidssted" }}</div>
                    <div>{{ hentTekst "oppsummering.fastlege.telefonnummer" }}: {{ hentFaktumValue "fastlege.annen.behandlende.tlfnr" }}</div>
                </td>
            </tr>
        {{else}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <div>{{ hentTekst "oppsummering.fastlege.navn" }}: {{ hentFaktumValue "fastlege.navn" }}</div>
                    <div>{{ hentTekst "oppsummering.fastlege.arbeidsstedadresse" }}: {{ hentFaktumValue "fastlege.arbeidsstedadresse" }}</div>
                    <div>{{ hentTekst "oppsummering.fastlege.telefonnummer" }}: {{ hentFaktumValue "fastlege.tlfnr" }}</div>
                </td>
            </tr>
            {{> skjema/bilstonad/fastlege/behandlendeLege }}
        {{/hvisSant}}
    </table>
{{else}}
    <h2>{{ hentTekst "fastlege.tittel" }}</h2>
    <table>
        <tr>
            <td class="sjekket"></td>
            <td>
                <div>{{ hentTekst "oppsummering.fastlege.navn" }}: {{ hentFaktumValue "fastlege.navn" }}</div>
                <div>{{ hentTekst "oppsummering.fastlege.arbeidsstedadresse" }}: {{ hentFaktumValue "fastlege.arbeidsstedadresse" }}</div>
                <div>{{ hentTekst "oppsummering.fastlege.telefonnummer" }}: {{ hentFaktumValue "fastlege.tlfnr" }}</div>
            </td>
        </tr>
        {{> skjema/bilstonad/fastlege/behandlendeLege }}
    </table>
{{/forFaktum}}