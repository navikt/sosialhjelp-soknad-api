{{> skjema/generisk/sporsmal}}
{{> skjema/generisk/infotekst}}

<ul class="svar-liste">
    <li>
        {{#hvisIkkeTom value}}
            <span class="verdi verdi--radio">
                {{ hentTekst (concat key "." value) }}
            </span>
        {{else}}
            <span class="verdi verdi--ikkeUtfylt">
                {{#if optional}}
                    <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                {{/if}}
            </span>
        {{/hvisIkkeTom}}
        {{#if @root.utvidetSoknad}}
            {{#hvisIkkeTom (hentSvaralternativer key)}}
                <p>
                    <h{{niva}}>Svaralternativer:</h{{niva}}>
                    <strong>
                        {{hentSvaralternativer key}}
                    </strong>
                </p>
            {{/hvisIkkeTom}}
            {{#hvisLik key 'familie.barn.true.barnebidrag'}}
                <p>
                    <h{{niva}}>Svaralternativer:</h{{niva}}>
                    <strong>
                        1) Jeg betaler barnebidrag 2) Jeg mottar barnebidrag 3) Jeg betaler og
                        mottar
                        barnebidrag 4) Ingen av delene
                    </strong>
                </p>
            {{/hvisLik}}
        {{/if}}
        {{> skjema/generisk/infotekstvalue}}
        {{> skjema/generisk/barnefakta}}
    </li>
</ul>

{{> skjema/generisk/vedlegg}}