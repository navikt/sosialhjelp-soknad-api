<h2>{{hentTekst "infofar.tittel"}}</h2>
<table>
    <tr>
        <td class="sjekket"></td>
        <td>
            <span>{{hentFaktumValue "infofar.opplysninger.fornavn"}} {{hentFaktumValue "infofar.opplysninger.etternavn"}},
                {{#forFaktum "infofar.opplysninger.personinfo"}}
                    {{#hvisLik "fodselsdato" properties.valg}}{{formatterLangDato properties.fodselsdato}}{{/hvisLik}}
                    {{#hvisLik "personnummer" properties.valg}}{{properties.personnummer}}{{/hvisLik}}
                {{/forFaktum}}
            </span>
        </td>
    </tr>
    <tr>
        <td class="sjekket"></td>
        <td>
            {{#forFaktum "rettigheter.omsorg"}}
                {{#hvisLik "morfarborsammen" value}}
                    <span>{{hentTekst "infofar.aleneomsorg.alternativ.1"}}</span>{{/hvisLik}}
                {{#hvisLik "aleneomsorg" value}}
                    <span>{{hentTekst "infofar.aleneomsorg.alternativ.2"}}</span>
                    <div>{{hentFaktumValue "infofar.aleneomsorg.fritekst"}}</div>
                {{/hvisLik}}
            {{/forFaktum}}
        </td>
    </tr>
    {{#forFaktum "rettighet.harrett"}}
        {{#hvisIkkeTom value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisLik "true" value}}<span>{{hentTekst "infofar.farrett.true"}}</span>{{/hvisLik}}
                    {{#hvisLik "false" value}}
                        <span>{{hentTekst "infofar.farrett.false"}}</span>
                        <div>{{hentFaktumValue "infofar.farrett.fritekst"}}</div>
                    {{/hvisLik}}
                </td>
            </tr>
        {{/hvisIkkeTom}}
    {{/forFaktum}}
    {{#forFaktum "infofar.informert"}}
        {{#hvisIkkeTom value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    {{#hvisLik "true" value}}<span>{{hentTekst "infofar.informert.true"}}</span>{{/hvisLik}}
                    {{#hvisLik "false" value}}
                        <span>{{hentTekst "infofar.informert.false"}}</span>
                        <div>{{hentFaktumValue "infofar.informert.fritekst"}}</div>
                    {{/hvisLik}}
                </td>
            </tr>
        {{/hvisIkkeTom}}
    {{/forFaktum}}
</table>