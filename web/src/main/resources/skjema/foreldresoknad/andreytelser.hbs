<h2>{{ hentTekst "andreytelser.tittel" }}</h2>
<table>
    {{#forFaktum "ytelser.ventelonn"}}
        {{#hvisLik "true" value}}

            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "ytelser.ventelonn.label" }}</span>
                </td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}
    {{#forFaktum "ytelser.lonnarbeidsgiver"}}
        {{#hvisLik "true" value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "ytelser.lonnarbeidsgiver.label" }}</span>
                </td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}
    {{#forFaktum "ytelser.vartpenger"}}
        {{#hvisLik "true" value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "ytelser.vartpenger.label" }}</span>
                </td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}
    {{#forFaktum "ytelser.avtjening"}}
        {{#hvisLik "true" value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "ytelser.avtjening.label" }}</span>
                </td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}
    {{#forFaktum "ytelser.ingenYtelse"}}
        {{#hvisLik "true" value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "ytelser.ingenYtelse.label" }}</span>
                </td>
            </tr>
        {{/hvisLik}}
    {{/forFaktum}}

    {{#forFaktum "ytelser.sluttpakke"}}
        {{#hvisSant value}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "andreytelser.sluttpakke.true" }}</span>
                </td>
            </tr>
        {{else}}
            <tr>
                <td class="sjekket"></td>
                <td>
                    <span>{{ hentTekst "andreytelser.sluttpakke.false" }}</span>
                </td>
            </tr>
        {{/hvisSant}}
    {{/forFaktum}}
</table>
