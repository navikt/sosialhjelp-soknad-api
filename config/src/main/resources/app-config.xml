<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://appconfig.aura.nav.no"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://appconfig.aura.nav.no
             http://maven.adeo.no/xsd/appconfig.xsd ">

    <name>soknad</name>

    <selftestPagePath>soknad/internal/selftest?status</selftestPagePath>

    <artifacts>
        <ear groupId="no.nav.sbl.dialogarena" artifactId="soknad-jee"/>
    </artifacts>

    <resources>
        <webservice alias="domene.Brukerdialog:sendsoknadservice_v1"
                    mapToProperty="soknad.webservice.henvendelse.sendsoknadservice"/>
        <baseUrl alias="footer.kontakt" mapToProperty="footer.kontakt"/>
        <baseUrl alias="footer.personvern" mapToProperty="footer.personvern"/>
        <baseUrl alias="footer.feilOgMangler" mapToProperty="footer.feilOgMangler"/>
        <baseUrl alias="footer.tilgjengelighet" mapToProperty="footer.tilgjengelighet"/>
        <baseUrl alias="footer.nettstedskart" mapToProperty="footer.nettstedskart"/>
        <baseUrl alias="soknad.navigasjonslink" mapToProperty="soknad.navigasjonslink"/>
        <baseUrl alias="soknad.logoutURL" mapToProperty="soknad.logoutURL"/>
        <applicationCertificate alias="srvSoknad"/>
    </resources>
    <security serviceUserResourceAlias="srvSoknad">
        <logins>
            <openAm>
                <contextRoot>soknad</contextRoot>
            </openAm>
        </logins>
    </security>
</application>