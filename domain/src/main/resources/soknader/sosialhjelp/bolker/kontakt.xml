<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>kontakt.adresse.brukerendrettoggle</id>
        <panel>personaliaBolk</panel>
        <type>hidden</type>
    </faktum>
    <faktum>
        <id>kontakt.system.adresse</id>
        <panel>personaliaBolk</panel>
        <type>composite</type>
        <constraints>
            <constraint>
                <faktum>kontakt.adresse.brukerendrettoggle</faktum>
                <expression>value != 'true'</expression>
            </constraint>
        </constraints>
        <properties>
            <property>
                <id>adresse</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'ustrukturert'</constraint>
                </constraints>
            </property>
            <property>
                <id>kommunenummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse' or properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>
                <id>bolignummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse' or properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>
                <id>postnummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse' or properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>
                <id>poststed</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse' or properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>	            
                <id>gatenavn</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>husnummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'gateadresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>eiendomsnavn</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>gaardsnummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>bruksnummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>festenummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>seksjonsnummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
            <property>              
                <id>undernummer</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['type'] == 'matrikkeladresse'</constraint>
                </constraints>
            </property>
        </properties>
    </faktum>
    <faktum>
        <id>kontakt.adresse.bruker</id>
        <panel>personaliaBolk</panel>
        <type>composite</type>
        <constraints>
            <constraint>
                <faktum>kontakt.adresse.brukerendrettoggle</faktum>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
        <properties>
            <property>
                <id>gateadresse</id>
                <type>textbox</type>
            </property>
            <property>
                <id>postnummer</id>
                <type>textbox</type>
            </property>
            <property>
                <id>poststed</id>
                <type>textbox</type>
            </property>
        </properties>
    </faktum>
    <faktum>
        <id>kontakt.system.kontonummer</id>
        <panel>personaliaBolk</panel>
        <type>textbox</type>
        <constraints>
            <constraint>
                <faktum>kontakt.kontonummer.brukerendrettoggle</faktum>
                <expression>value != 'true'</expression>
            </constraint>
        </constraints>
    </faktum>
    <faktum>
        <id>kontakt.kontonummer.brukerendrettoggle</id>
        <panel>personaliaBolk</panel>
        <type>hidden</type>
    </faktum>
    <faktum>
        <id>kontakt.kontonummer</id>
        <panel>personaliaBolk</panel>
        <type>textbox</type>
        <constraints>
            <constraint>
                <faktum>kontakt.kontonummer.brukerendrettoggle</faktum>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>kontakt.kontonummer.harikke</id>
        <panel>personaliaBolk</panel>
        <type>checkbox</type>
    </faktum>
    <faktum>
        <id>kontakt.system.telefon</id>
        <panel>personaliaBolk</panel>
        <type>textbox</type>
        <constraints>
            <constraint>
                <faktum>kontakt.telefon.brukerendrettoggle</faktum>
                <expression>value != 'true'</expression>
            </constraint>
        </constraints>
    </faktum>
    <faktum>
        <id>kontakt.telefon.brukerendrettoggle</id>
        <panel>personaliaBolk</panel>
        <type>hidden</type>
    </faktum>
    <faktum>
        <id>kontakt.telefon</id>
        <panel>personaliaBolk</panel>
        <type>textbox</type>
        <constraints>
            <constraint>
                <faktum>kontakt.telefon.brukerendrettoggle</faktum>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>kontakt.statsborger</id>
        <panel>personaliaBolk</panel>
        <type>bool</type>
        <optional>true</optional>
    </faktum>
</soknad>