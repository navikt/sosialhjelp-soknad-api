<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>utgifter.boutgift</id>
        <type>bool</type>
        <panel>utgifterBolk</panel>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type</id>
        <type>checkboxGroup</type>
        <dependOn>utgifter.boutgift</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.husleie</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.strom</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.kommunalAvgift</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.oppvarming</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.boliglanAvdrag</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.boutgift.true.type.annenBoutgift</id>
        <type>bool</type>
        <dependOn>utgifter.boutgift.true.type</dependOn>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>utgifter.boutgift.true.type.annenBoutgift.true.beskrivelse</id>
        <type>hidden</type>
        <dependOn>utgifter.boutgift.true.type.annenBoutgift</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>utgifter.barn</id>
        <type>bool</type>
        <panel>utgifterBolk</panel>
        <optional>true</optional>
        <constraints>
            <constraint>
                <faktum>system.familie.barn</faktum>
                <expression>value != "false"</expression>
            </constraint>
        </constraints>
    </faktum>

    <faktum>
        <id>utgifter.barn.true.utgifter</id>
        <type>checkboxGroup</type>
        <dependOn>utgifter.barn</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>utgifter.barn.true.utgifter.barnFritidsaktiviteter</id>
        <type>bool</type>
        <dependOn>utgifter.barn.true.utgifter</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.barn.true.utgifter.barnehage</id>
        <type>bool</type>
        <dependOn>utgifter.barn.true.utgifter</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.barn.true.utgifter.sfo</id>
        <type>bool</type>
        <dependOn>utgifter.barn.true.utgifter</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.barn.true.utgifter.barnTannregulering</id>
        <type>bool</type>
        <dependOn>utgifter.barn.true.utgifter</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.barn.true.utgifter.annenBarneutgift</id>
        <type>bool</type>
        <dependOn>utgifter.barn.true.utgifter</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>utgifter.barn.true.utgifter.annenBarneutgift.true.beskrivelse</id>
        <type>hidden</type>
        <dependOn>utgifter.barn.true.utgifter.annenBarneutgift</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>
</soknad>