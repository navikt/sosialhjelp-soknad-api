    <faktum>
        <id>opplysninger.arbeid.jobb</id>
        <type>composite</type>
        <dependOn>opplysninger.arbeid</dependOn>
        <constraints>
            <constraint>
                <faktum>dinsituasjon.jobb</faktum>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
        <properties>
            <property>
                <id>bruttolonn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>nettolonn</id>
                <type>textbox</type>
            </property>
        </properties>
        <optional>true</optional>
        <flereTillatt>true</flereTillatt>
    </faktum>

