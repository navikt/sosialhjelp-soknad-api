<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">

    <faktum>
        <id>navytelser</id>
        <panel>inntektBolk</panel>
    </faktum>

    <faktum>
        <id>utbetalinger</id>
        <panel>inntektBolk</panel>
    </faktum>

    <faktum>
        <id>utbetalinger.ingen</id>
        <panel>inntektBolk</panel>
        <type>hidden</type>
        <constraints>
            <constraint>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
    </faktum>

    <faktum>
        <id>utbetalinger.feilet</id>
        <panel>inntektBolk</panel>
        <type>hidden</type>
    </faktum>

    <faktum>
        <id>utbetalinger.utbetaling</id>
        <flereTillatt>true</flereTillatt>
        <erSystemFaktum>true</erSystemFaktum>
        <panel>inntektBolk</panel>
        <type>composite</type>
        <properties>
            <property>
                <id>utbetalingsid</id>
                <type>hidden</type>
            </property>
            <property>
                <id>type</id>
                <type>textbox</type>
            </property>
            <property>
                <id>utbetalingsDato</id>
                <type>textbox</type>
            </property>
            <property>
                <id>netto</id>
                <type>textbox</type>
            </property>
            <property>
                <id>brutto</id>
                <type>textbox</type>
            </property>
            <property>
                <id>skattetrekk</id>
                <type>textbox</type>
            </property>
            <property>
                <id>andretrekk</id>
                <type>textbox</type>
            </property>
            <property>
                <id>periodeFom</id>
                <type>hidden</type>
            </property>
            <property>
                <id>periodeTom</id>
                <type>hidden</type>
            </property>
        </properties>
    </faktum>
    <faktum>
        <id>utbetalinger.utbetaling.komponent</id>
        <flereTillatt>true</flereTillatt>
        <erSystemFaktum>true</erSystemFaktum>
        <panel>inntektBolk</panel>
        <type>hidden</type>
        <properties>
            <property>
                <id>utbetalingsid</id>
                <type>textbox</type>
            </property>
            <property>
                <id>type</id>
                <type>textbox</type>
            </property>
            <property>
                <id>belop</id>
                <type>textbox</type>
            </property>
            <property>
                <id>satstype</id>
                <type>textbox</type>
            </property>
            <property>
                <id>satsbelop</id>
                <type>textbox</type>
            </property>
            <property>
                <id>satsantall</id>
                <type>textbox</type>
            </property>
        </properties>
    </faktum>

    <faktum>
        <id>inntekt.bostotte</id>
        <type>bool</type>
        <panel>inntektBolk</panel>
        <optional>true</optional>
    </faktum>

    <!-- Eierandeler-->
    <faktum>
        <id>inntekt.eierandeler</id>
        <type>bool</type>
        <panel>inntektBolk</panel>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type</id>
        <type>checkboxGroup</type>
        <dependOn>inntekt.eierandeler</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type.bolig</id>
        <type>bool</type>
        <dependOn>inntekt.eierandeler.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type.kjoretoy</id>
        <type>bool</type>
        <dependOn>inntekt.eierandeler.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type.campingvogn</id>
        <type>bool</type>
        <dependOn>inntekt.eierandeler.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type.fritidseiendom</id>
        <type>bool</type>
        <dependOn>inntekt.eierandeler.true.type</dependOn>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>inntekt.eierandeler.true.type.annet</id>
        <type>bool</type>
        <dependOn>inntekt.eierandeler.true.type</dependOn>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>inntekt.eierandeler.true.type.annet.true.beskrivelse</id>
        <type>textbox</type>
        <dependOn>inntekt.eierandeler.true.type.annet</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>

