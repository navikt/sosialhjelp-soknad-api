<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>familie.sivilstatus</id>
        <type>radio</type>
        <panel>familieBolk</panel>
    </faktum>
    <faktum>
        <id>familie.sivilstatus.gift.ektefelle</id>
        <type>composite</type>
        <dependOn>familie.sivilstatus</dependOn>
        <dependOnValues>
            <value>gift</value>
        </dependOnValues>
        <properties>
            <property>
                <id>navn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>pnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>borsammen</id>
                <type>bool</type>
            </property>
            <property>
                <id>ikkesammenbeskrivelse</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['borsammen'] == 'true'</constraint>
                </constraints>
            </property>
        </properties>
    </faktum>

    <faktum>
        <id>familie.barn</id>
        <type>bool</type>
        <panel>familieBolk</panel>
    </faktum>
    <faktum>
        <id>familie.barn.true.barn</id>
        <dependOn>familie.barn</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <type>composite</type>
        <flereTillatt>true</flereTillatt>
        <properties>
            <property>
                <id>navn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>pnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>borsammen</id>
                <type>bool</type>
            </property>
            <property>
                <id>grad</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['borsammen'] == 'true'</constraint>
                </constraints>
            </property>
        </properties>
    </faktum>
</soknad>