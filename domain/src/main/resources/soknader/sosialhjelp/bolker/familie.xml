<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>familie.sivilstatus</id>
        <type>radio</type>
        <constraints>
            <constraint>
                <faktum>familie.sivilstatus.sivilstatusOverskrivesAvBruker</faktum>
                <expression>value == 'true'</expression>
            </constraint>
        </constraints>
        <panel>familieBolk</panel>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>familie.sivilstatus.gift.ektefelle</id>
        <type>composite</type>
        <dependOn>familie.sivilstatus</dependOn>
        <dependOnValues>
            <value>gift</value>
        </dependOnValues>
        <properties>
            <property>
                <id>fornavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>mellomnavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>etternavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>pnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>borsammen</id>
                <type>bool</type>
            </property>
            <property>
                <id>ikkesammenbeskrivelse</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['borsammen'] == 'false'</constraint>
                </constraints>
            </property>
        </properties>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>familie.sivilstatus.sivilstatusOverskrivesAvBruker</id>
        <panel>familieBolk</panel>
        <type>hidden</type>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>system.familie.sivilstatus</id>
        <type>radio</type>
        <panel>familieBolk</panel>
        <constraints>
            <constraint>
                <faktum>familie.sivilstatus.sivilstatusOverskrivesAvBruker</faktum>
                <expression>value != 'true'</expression>
            </constraint>
        </constraints>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>system.familie.sivilstatus.gift.ektefelle</id>
        <type>composite</type>
        <dependOn>system.familie.sivilstatus</dependOn>
        <dependOnValues>
            <value>gift</value>
        </dependOnValues>
        <properties>
            <property>
                <id>fornavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>mellomnavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>etternavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fodselsdato</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>hidden</type>
            </property>
            <property>
                <id>folkeregistrertsammen</id>
                <type>bool</type>
            </property>
            <property>
                <id>ikketilgangtilektefelle</id>
                <type>hidden</type>
            </property>
        </properties>
        <optional>true</optional>
    </faktum>

    <faktum>
        <id>familie.barn</id>
        <type>bool</type>
        <panel>familieBolk</panel>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>familie.barn.true.barnebidrag</id>
        <dependOn>familie.barn</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <type>radio</type>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>familie.barn.true.barn</id>
        <dependOn>familie.barn</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <type>composite</type>
        <flereTillatt>true</flereTillatt>
        <properties>
            <property>
                <id>fornavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>mellomnavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>etternavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>pnr</id>
                <type>textbox</type>
            </property>
            <property>
                <id>borsammen</id>
                <type>bool</type>
            </property>
            <property>
                <id>grad</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['borsammen'] == 'false'</constraint>
                </constraints>
            </property>
        </properties>
        <optional>true</optional>
    </faktum>
</soknad>