<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>dinsituasjon.jobb</id>
        <type>bool</type>
        <panel>arbeidbolk</panel>
        <optional>true</optional>
        <constraints>
            <constraint>
                <faktum>dinsituasjon.registrertjobb</faktum>
                <expression>properties['harhentetfraaareg'] != 'true'</expression>
            </constraint>
        </constraints>
    </faktum>

    <faktum>
        <id>dinsituasjon.registrertjobb</id>
        <type>hidden</type>
        <panel>arbeidbolk</panel>
        <optional>true</optional>
        <erSystemFaktum>true</erSystemFaktum>
    </faktum>


    <faktum>
        <id>dinsituasjon.studerer</id>
        <type>bool</type>
        <panel>arbeidbolk</panel>
        <optional>true</optional>
    </faktum>
    <faktum>
        <id>dinsituasjon.studerer.true.grad</id>
        <type>bool</type>
        <dependOn>dinsituasjon.studerer</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
        <optional>true</optional>
    </faktum>


    <faktum>
        <id>arbeidsforhold</id>
        <type>composite</type>

        <panel>arbeidbolk</panel>
        <properties>
            <property>
                <id>arbeidsgivernavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fom</id>
                <type>textbox</type>
            </property>
            <property>
                <id>tom</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['tom'] != null and properties['tom'] != ''</constraint>
                </constraints>
            </property>
            <property>
                <id>stillingsprosent</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['stillingsprosent'] != '0'</constraint>
                </constraints>
            </property>
            <property>
                <id>stillingstype</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['stillingstype'] == 'variabel'</constraint>
                </constraints>
            </property>
        </properties>
        <constraints>
            <constraint>
                <expression>properties['fom'] != null</expression>
            </constraint>
        </constraints>
        <optional>true</optional>
        <flereTillatt>true</flereTillatt>
    </faktum>

    <faktum>
        <id>opplysninger.arbeidsituasjon.kommentarer</id>
        <panel>arbeidbolk</panel>
        <type>textbox</type>

    </faktum>

</soknad>