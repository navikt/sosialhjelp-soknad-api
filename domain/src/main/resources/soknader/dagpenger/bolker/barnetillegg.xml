<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>barn</id>
        <panel>Barnetillegg</panel>
        <flereTillatt>true</flereTillatt>
        <type>composite</type>

        <constraints>
            <constraint>
                <expression>properties['barnetillegg'] == 'true'</expression>
            </constraint>
        </constraints>

        <properties>
            <property>
                <id>barnetillegg</id>
                <type>hidden</type>
            </property>
            <property>
                <id>fornavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>etternavn</id>
                <type>textbox</type>
            </property>
            <property>
                <id>fnr</id>
                <type>textbox</type>
                <constraints>
                    <constraint>type == 'SYSTEMREGISTRERT'</constraint>
                </constraints>
            </property>
            <property>
                <id>fodselsdato</id>
                <type>date</type>
                <constraints>
                    <constraint>type == 'BRUKERREGISTRERT'</constraint>
                </constraints>
            </property>
            <property>
                <id>land</id>
                <type>country</type>
            </property>
            <property>
                <id>ikkebarneinntekt</id>
                <type>java.lang.Boolean</type>
            </property>
            <property>
                <id>barneinntekttall</id>
                <type>textbox</type>
                <constraints>
                    <constraint>properties['ikkebarneinntekt'] == 'false'</constraint>
                </constraints>
            </property>
            <property>
                <id>ikkeeos</id>
                <type>infotekst</type>
                <constraints>
                    <constraint>properties['landtype'] == 'ikkeEos'</constraint>
                </constraints>
            </property>
            <property>
                <id>eos</id>
                <type>infotekst</type>
                <constraints>
                    <constraint>properties['landtype'] == 'eos'</constraint>
                </constraints>
            </property>
        </properties>
    </faktum>



    <vedlegg>
        <faktum>barn</faktum>
        <skjemaNummer>X8</skjemaNummer>
        <property>sammensattnavn</property>
        <onProperty>vedlegg</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <forSystemfaktum>false</forSystemfaktum>
        <flereTillatt>true</flereTillatt>
    </vedlegg>
</soknad>