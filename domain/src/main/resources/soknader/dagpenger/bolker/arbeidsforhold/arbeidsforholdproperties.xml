<?xml version="1.0" ?>
<properties>
    <property>
        <id>arbeidsgivernavn</id>
        <type>textbox</type>
    </property>
    <property>
        <id>land</id>
        <type>country</type>
    </property>
    <property>
        <id>type</id>
        <type>radio</type>
    </property>
    <property>
        <id>datofra</id>
        <type>date</type>
        <constraints>
            <constraint>properties['type'] == 'permittert'</constraint>
        </constraints>
    </property>
    <property>
        <id>startperiode</id>
        <type>date</type>
        <constraints>
            <constraint>properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
    <property>
        <id>redusertfra</id>
        <type>date</type>
        <constraints>
            <constraint>properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
    <property>
        <id>varighet</id>
        <type>periode</type>
        <constraints>
            <constraint>properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
            <constraint>properties['type'] == 'kontraktutgaatt'</constraint>
            <constraint>properties['type'] == 'sagtoppselv'</constraint>
        </constraints>
        <configuration>
            <entry>
                <key>fom</key>
                <value>datofra</value>
            </entry>
            <entry>
                <key>tom</key>
                <value>datotil</value>
            </entry>
        </configuration>
    </property>
    <property>
        <id>varighet</id>
        <type>periode</type>
        <constraints>
            <constraint>properties['type'] == 'arbeidsgivererkonkurs'</constraint>
        </constraints>
        <configuration>
            <entry>
                <key>fom</key>
                <value>datofra</value>
            </entry>
            <entry>
                <key>tom</key>
                <value>konkursdato</value>
            </entry>
        </configuration>
    </property>
    <property>
        <id>aarsak</id>
        <type>textbox</type>
        <constraints>
            <constraint>properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
        </constraints>
    </property>
    <property>
        <id>bostyrersnavn</id>
        <type>textbox</type>
        <constraints>
            <constraint>properties['type'] == 'arbeidsgivererkonkurs'</constraint>
        </constraints>
    </property>
    <property>
        <id>sagtoppselvAarsak</id>
        <type>textbox</type>
        <constraints>
            <constraint>properties['type'] == 'sagtoppselv'</constraint>
        </constraints>
    </property>
    <property>
        <id>nyttTilbud</id>
        <type>radio</type>
        <constraints>
            <constraint>properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
    <property>
        <id>lonnsgaranti</id>
        <type>radio</type>
        <constraints>
            <constraint>properties['type'] == 'arbeidsgivererkonkurs'</constraint>
        </constraints>
    </property>
    <property>
        <id>lonnsgarantidekker</id>
        <type>radio</type>
        <constraints>
            <constraint>properties['type'] == 'arbeidsgivererkonkurs' &amp;&amp; properties['lonnsgaranti'] ==
                'JaHarSokt'
            </constraint>
        </constraints>
    </property>
    <property>
        <id>lonnkonkursmaaned</id>
        <type>radio</type>
        <constraints>
            <constraint>properties['type'] == 'arbeidsgivererkonkurs'</constraint>
        </constraints>
    </property>
    <property>
        <id>sagtoppavarbeidsgiver.tilbudomjobbannetsted</id>
        <type>java.lang.Boolean</type>
        <constraints>
            <constraint>properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
        </constraints>
    </property>
    <property>
        <id>tilbudomjobbannetsted</id>
        <constraints>
            <constraint>properties['type'] == 'kontraktutgaatt'</constraint>
        </constraints>
    </property>
    <property>
        <id>lonnspliktigperiodevetikke</id>
        <type>checkbox</type>
        <constraints>
            <constraint>properties['type'] == 'permittert' &amp;&amp; properties['lonnspliktigperiodevetikke']
                == 'true'
            </constraint>
        </constraints>
    </property>
    <property>
        <id>lonnsplikt</id>
        <type>periode</type>
        <constraints>
            <constraint>properties['type'] == 'permittert' &amp;&amp; properties['lonnspliktigperiodevetikke']
                != 'true'
            </constraint>
        </constraints>
        <configuration>
            <entry>
                <key>fom</key>
                <value>lonnspliktigperiodedatofra</value>
            </entry>
            <entry>
                <key>tom</key>
                <value>lonnspliktigperiodedatotil</value>
            </entry>
        </configuration>
    </property>
    <property>
        <id>rotasjonskiftturnus</id>
        <type>radio</type>
        <constraints>
            <constraint>properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
            <constraint>properties['type'] == 'permittert'</constraint>
            <constraint>properties['type'] == 'kontraktutgaatt'</constraint>
            <constraint>properties['type'] == 'sagtoppselv'</constraint>
            <constraint>properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
    <property>
        <id>arbeidsdager</id>
        <type>textbox</type>
        <constraints>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'permittert'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'kontraktutgaatt'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'sagtoppselv'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
    <property>
        <id>avspaseringsdager</id>
        <type>textbox</type>
        <constraints>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'sagtoppavarbeidsgiver'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'permittert'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'kontraktutgaatt'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'sagtoppselv'</constraint>
            <constraint>properties['rotasjonskiftturnus'] == 'jarotasjon' &amp;&amp; properties['type'] == 'redusertarbeidstid'</constraint>
        </constraints>
    </property>
</properties>
