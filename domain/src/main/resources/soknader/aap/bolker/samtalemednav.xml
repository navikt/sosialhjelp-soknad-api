<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <!-- Hovedvisning -->
    <faktum>
        <id>samtalemednav.hjelpefaktum</id>
        <type>hidden</type>
    </faktum>
    <faktum>
        <id>samtalemednav</id>
        <type>string</type>
        <panel>samtalemednav</panel>
        <constraints>
            <constraint>
                <faktum>samtalemednav.hjelpefaktum</faktum>
                <expression>value == 'visarbeidsmuligheter'</expression>
            </constraint>
        </constraints>
    </faktum>

    <faktum>
        <id>samtalemednav.arbeidsmuligheter</id>
        <type>string</type>
        <dependOn>samtalemednav</dependOn>
        <dependOnValues>
            <value>harikkehattsamtale</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidserfaring.godearbeidssituasjoner</id>
        <type>textbox</type>
        <dependOn>samtalemednav.arbeidsmuligheter</dependOn>
        <dependOnValues>
            <value>harikkejobb</value>
            <value>ikketilbaketilnavaerendejobb</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidserfaring.arbeidsoppgaver</id>
