<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <!-- Hovedvisning -->
    <faktum>
        <id>samtalemednav</id>
        <type>java.lang.String</type>
        <panel>samtalemednav</panel>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidsmuligheter</id>
        <type>java.lang.String</type>
        <dependOn>samtalemednav</dependOn>
        <dependOnValues>
            <value>harikkehattsamtale</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidserfaring.godearbeidssituasjoner</id>
        <type>textbox</type>
        <dependOn>samtalemednav.arbeidsmuligheter</dependOn>
        <dependOnValues>
            <value>harikkejobb</value>
            <value>ikketilbaketilnavaerendejobb</value>
        </dependOnValues>
        <constraints>
            <constraint>
                <faktum>samtalemednav</faktum>
                <expression>value == "harikkehattsamtale"</expression>
            </constraint>
        </constraints>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidserfaring.arbeidsoppgaver</id>
        <type>textbox</type>
        <dependOn>samtalemednav.arbeidsmuligheter</dependOn>
        <dependOnValues>
            <value>harikkejobb</value>
            <value>ikketilbaketilnavaerendejobb</value>
        </dependOnValues>
        <constraints>
            <constraint>
                <faktum>samtalemednav</faktum>
                <expression>value == "harikkehattsamtale"</expression>
            </constraint>
        </constraints>
    </faktum>
    <faktum>
        <id>samtalemednav.arbeidserfaring.fremtidigjobb</id>
        <type>textbox</type>
        <dependOn>samtalemednav.arbeidsmuligheter</dependOn>
        <dependOnValues>
            <value>harikkejobb</value>
            <value>ikketilbaketilnavaerendejobb</value>
        </dependOnValues>
        <constraints>
            <constraint>
                <faktum>samtalemednav</faktum>
                <expression>value == "harikkehattsamtale"</expression>
            </constraint>
        </constraints>
    </faktum>
    <faktum>
        <id>samtalen.kunnskapogegenskaper</id>
    </faktum>
    <faktum>
        <id>samtalen.helsesituasjon</id>
    </faktum>
    <faktum>
        <id>samtalen.andreforhold</id>
    </faktum>

    <!-- Studenter -->
    <faktum>
        <id>samtalen.student.tilbaketilstudiet.samtalemednav</id>
    </faktum>
    <faktum>
        <id>samtalen.student.ikketilbaketilstudiet.samtalemednav</id>
    </faktum>
    <faktum>
        <id>samtalen.student.arbeidserfaring</id>
    </faktum>
    <faktum>
        <id>samtalen.student.kunnskapogegenskaper</id>
    </faktum>
    <faktum>
        <id>samtalen.student.helsesituasjon</id>
    </faktum>
    <faktum>
        <id>samtalen.student.andreforhold</id>
    </faktum>
</soknad>