<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl">
    <faktum>
        <id>perioder.startdato</id>
    </faktum>
    <faktum>
        <id>perioder.permisjon</id>
    </faktum>
    <faktum>
        <id>perioder.endring.omsorg</id>
    </faktum>

    <faktum>
        <id>perioder.tidsrom.utsettelse</id>
        <flereTillatt>true</flereTillatt>
        <dependOn>perioder.opphold</dependOn>
        <dependOnValues>
            <value>true</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>perioder.opphold</id>
        <flereTillatt>true</flereTillatt>
    </faktum>

    <faktum>
        <id>perioder.morsaktivitet</id>
        <flereTillatt>true</flereTillatt>
    </faktum>

    <!-- Vedlegg perioder -->
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <onProperty>graderingsskjema</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>H1</skjemaNummer>
    </vedlegg>

    <vedlegg>
        <faktum>perioder.tidsrom.utsettelse</faktum>
        <skjemanummerTillegg>ferie</skjemanummerTillegg>
        <filterKey>type</filterKey>
        <filterValues>
            <value>ferie</value>
        </filterValues>
        <skjemaNummer>Z6</skjemaNummer>
        <property>type</property>
        <onProperty>ferie</onProperty>
        <vedleggTittel>perioder.leggtil.utsettelse.ferie.vedleggtittel</vedleggTittel>
    </vedlegg>
    <vedlegg>
        <faktum>perioder.tidsrom.utsettelse</faktum>
        <skjemanummerTillegg>arbeid</skjemanummerTillegg>
        <filterKey>type</filterKey>
        <filterValues>
            <value>arbeid</value>
        </filterValues>
        <skjemaNummer>Z6</skjemaNummer>
        <property>type</property>
        <onProperty>arbeid</onProperty>
        <vedleggTittel>perioder.leggtil.utsettelse.arbeid.vedleggtittel</vedleggTittel>
    </vedlegg>
    <vedlegg>
        <faktum>perioder.tidsrom.utsettelse</faktum>
        <filterKey>type</filterKey>
        <filterValues>
            <value>sykdom</value>
        </filterValues>
        <skjemaNummer>L9</skjemaNummer>
        <skjemanummerTillegg>utsettelse</skjemanummerTillegg>
        <property>type</property>
        <onProperty>sykdom</onProperty>
        <vedleggTittel>perioder.leggtil.utsettelse.sykdom.vedleggtittel</vedleggTittel>
    </vedlegg>

    <vedlegg>
        <faktum>perioder.tidsrom.utsettelse</faktum>
        <filterKey>type</filterKey>
        <filterValues>
            <value>innlagt</value>
        </filterValues>
        <skjemaNummer>N9</skjemaNummer>
        <skjemanummerTillegg>innlagt</skjemanummerTillegg>
        <property>type</property>
        <onProperty>innlagt</onProperty>
        <vedleggTittel>perioder.leggtil.utsettelse.innlagt.vedleggtittel</vedleggTittel>
    </vedlegg>
    <vedlegg>
        <faktum>perioder.tidsrom.utsettelse</faktum>
        <filterKey>type</filterKey>
        <filterValues>
            <value>barninnlagt</value>
        </filterValues>
        <skjemaNummer>N9</skjemaNummer>
        <skjemanummerTillegg>barninnlagt</skjemanummerTillegg>
        <property>type</property>
        <onProperty>barninnlagt</onProperty>
        <vedleggTittel>perioder.leggtil.utsettelse.barninnlagt.vedleggtittel</vedleggTittel>
    </vedlegg>

    <!-- Vedlegg mors aktivitet -->
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <skjemanummerTillegg>morarbeid</skjemanummerTillegg>
        <property>morarbeidVedleggtittel</property>
        <onProperty>morarbeid</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>Z6</skjemaNummer>
    </vedlegg>

    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <property>morstudierVedleggtittel</property>
        <onProperty>morstudier</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>O9</skjemaNummer>
    </vedlegg>
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <property>morsykVedleggtittel</property>
        <onProperty>morsyk</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>L9</skjemaNummer>
        <skjemanummerTillegg>aktivitetskrav</skjemanummerTillegg>
    </vedlegg>
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <skjemanummerTillegg>morinnlagt</skjemanummerTillegg>
        <property>morinnlagtVedleggtittel</property>
        <onProperty>morinnlagt</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>N9</skjemaNummer>
    </vedlegg>
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <property>introduksjonsprogramVedleggtittel</property>
        <onProperty>introduksjonsprogram</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>T1</skjemaNummer>
    </vedlegg>
    <vedlegg>
        <faktum>vedlegg.hjelpefaktum</faktum>
        <property>kvalifiseringsprogramVedleggtittel</property>
        <onProperty>kvalifiseringsprogram</onProperty>
        <onValues>
            <value>true</value>
        </onValues>
        <skjemaNummer>K3</skjemaNummer>
    </vedlegg>

</soknad>