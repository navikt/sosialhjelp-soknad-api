<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <defaultCache eternal="true" maxElementsInMemory="100" overflowToDisk="false"/>

    <!--
         Security Tokens are not writable to Disk so use a memory only
         cache with a bit larger maxEntriesLocalHeap.  However, this
         cache is per-endpoint so not "too large" to not consume the
         entire heap.
     -->
    <cache name="org.apache.cxf.ws.security.tokenstore.TokenStore"
           overflowToDisk="false"
           maxEntriesLocalHeap="10000"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
            />

    <diskStore path="/tmp/default/path"/>

    <cache name="dkifCache" maxElementsInMemory="100" overflowToDisk="false" timeToLiveSeconds="1800"  />
    <cache name="kontonummerCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />
    <cache name="utbetalingCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="1800"  />
    <cache name="kommuneinfoCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="1800"  />
    <cache name="kommunesokCache" maxElementsInMemory="100" overflowToDisk="false" timeToLiveSeconds="86400"  />
    <cache name="pdlPersonCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />
    <cache name="pdlBarnCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />
    <cache name="pdlEktefelleCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />
    <cache name="pdlAdressebeskyttelseCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />
    <cache name="kontonummerOppslagCache" maxElementsInMemory="1000" overflowToDisk="false" timeToLiveSeconds="3600"  />

</ehcache>