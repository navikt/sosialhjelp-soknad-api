<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <session-config>
        <session-timeout>60</session-timeout>
        <cookie-config>
            <name>SENDSOKNAD_JSESSIONID</name>
            <http-only>true</http-only>
            <secure>false</secure>
            <path>/</path>
        </cookie-config>
        <tracking-mode>COOKIE</tracking-mode>
    </session-config>

    <filter>
        <filter-name>OpenAmLoginFilter</filter-name>
        <filter-class>no.nav.sosialhjelp.soknad.web.saml.FakeLoginFilter</filter-class>
        <init-param>
            <param-name>defaultFnr</param-name>
            <param-value>11111111111</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>OpenAmLoginFilter</filter-name>
        <url-pattern>/metadata/*</url-pattern>
    </filter-mapping>
</web-app>