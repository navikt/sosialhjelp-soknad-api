<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl" xmlns:xi="http://www.w3.org/2001/XInclude">

    <faktum>
        <id>arbeidsforhold</id>
        <panel>Arbeidsforhold</panel>
        <flereTillatt>true</flereTillatt>
        <type>composite</type>
        <xi:include href="arbeidsforhold/arbeidsforholdproperties.xml" />
    </faktum>
    <xi:include href="arbeidsforhold/permitteringsperiode.xml" />
    <xi:include href="arbeidsforhold/vedlegg.xml" />


    <faktum>
        <id>arbeidsforhold.datodagpenger</id>
        <type>textbox</type>
        <panel>Arbeidsforhold</panel>
    </faktum>
    <faktum>
        <id>arbeidsforhold.arbeidstilstand</id>
        <type>java.lang.String</type>
        <panel>Arbeidsforhold</panel>
    </faktum>
    <faktum>
        <id>harIkkeJobbet</id>
        <panel>Arbeidsforhold</panel>
    </faktum>

    <faktum>
        <id>arbeidsforhold.grensearbeider</id>
        <panel>Arbeidsforhold</panel>
        <type>java.lang.Boolean</type>
        <constraints>
            <constraint>
                <faktum>personalia</faktum>
                <expression>properties['statsborgerskap'] != 'NOR'</expression>
            </constraint>
        </constraints>
    </faktum>
    <xi:include href="arbeidsforhold/grensearbeider.xml" />

</soknad>