<?xml version="1.0" ?>
<soknad xmlns="urn:no.nav.sbl" xmlns:xi="http://www.w3.org/2001/XInclude">


    <faktum>
        <id>nyearbeidsforhold.arbeidsidensist</id>
        <type>java.lang.Boolean</type>
        <panel>NyeArbeidsforhold</panel>
    </faktum>

    <faktum>
        <id>nyearbeidsforhold.aarsakdagpengerstanset</id>
        <type>textbox</type>
        <dependOn>nyearbeidsforhold.arbeidsidensist</dependOn>
        <dependOnValues>
            <value>true</value>
            <value>false</value>
        </dependOnValues>
    </faktum>

    <faktum>
        <id>nyearbeidsforhold.datogjenopptak</id>
        <type>date</type>
        <dependOn>nyearbeidsforhold.arbeidsidensist</dependOn>
        <dependOnValues>
            <value>true</value>
            <value>false</value>
        </dependOnValues>
    </faktum>


    <faktum>
        <id>arbeidsforhold</id>
        <panel>NyeArbeidsforhold</panel>
        <flereTillatt>true</flereTillatt>
        <type>composite</type>
        <dependOn>nyearbeidsforhold.arbeidsidensist</dependOn>
        <dependOnValues>
            <value>false</value>
        </dependOnValues>
        <xi:include href="arbeidsforhold/arbeidsforholdproperties.xml" />
    </faktum>
    <xi:include href="arbeidsforhold/permitteringsperiode.xml" />
    <xi:include href="arbeidsforhold/vedlegg.xml" />


    <faktum>
        <id>nyearbeidsforhold.reberegning</id>
        <type>java.lang.Boolean</type>
        <dependOn>nyearbeidsforhold.arbeidsidensist</dependOn>
        <dependOnValues>
            <value>false</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>nyearbeidsforhold.reberegning.startdato</id>
        <type>java.lang.String</type>
        <dependOn>nyearbeidsforhold.reberegning</dependOn>
        <dependOnValues>
            <value>false</value>
        </dependOnValues>
    </faktum>
    <faktum>
        <id>nyearbeidsforhold.reberegning.arbeidstid</id>
        <type>java.lang.Boolean</type>
        <dependOn>nyearbeidsforhold.reberegning</dependOn>
        <dependOnValues>
            <value>false</value>
        </dependOnValues>
    </faktum>


    <faktum>
        <id>arbeidsforhold.grensearbeider</id>
        <panel>NyeArbeidsforhold</panel>
        <type>java.lang.Boolean</type>
        <constraints>
            <constraint>
                <faktum>personalia</faktum>
                <expression>properties['statsborgerskap'] != 'NOR'</expression>
            </constraint>
        </constraints>
    </faktum>
    <xi:include href="arbeidsforhold/grensearbeider.xml" />

</soknad>