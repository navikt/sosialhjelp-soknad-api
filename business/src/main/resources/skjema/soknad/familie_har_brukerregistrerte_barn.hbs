{{#if @root.utvidetSoknad}}
    <h3>{{hentTekst "familierelasjon.faktum.leggtil.sporsmal"}}</h3>
    {{{settInnHjelpetekst "familie.barn.true.barn.deltbosted.hjelpetekst.tekst"}}}
{{else}}
    <h3>{{hentTekst "familierelasjon.faktum.lagttil"}}</h3>
{{/if}}
{{#each forsorgerplikt.ansvar}}
    {{#hvisLik barn.kilde "bruker"}}
        <h3>{{ hentTekst "familie.barn.true.listetittel" }}</h3>
        {{#with barn}}
            <li>
                <span class="label">{{ hentTekst "kontakt.system.personalia.navn" }}</span>
                <ul class="svar-liste">
                    <li>
                        <span class="verdi verdi--tekst">
                            {{navn.fornavn}} {{navn.mellomnavn}} {{navn.etternavn}}
                        </span>
                    </li>
                </ul>
            </li>
            {{#if fodselsdato}}
                <li>
                    <span class="label">{{ hentTekst "familierelasjon.fodselsdato" }}</span>
                    <ul class="svar-liste">
                        <li>
                            {{#hvisIkkeTom fodselsdato}}
                                <span class="verdi verdi--tekst">
                                    {{fodselsdato}}
                                </span>
                            {{else}}
                                <span class="verdi verdi--ikkeUtfylt">
                                    <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                                </span>
                            {{/hvisIkkeTom}}
                        </li>
                    </ul>
                </li>
            {{/if}}
        {{/with}}
        <li>
            <span class="label">{{ hentTekst "familie.barn.true.barn.borsammen.sporsmal" }}</span>
            <ul class="svar-liste">
                <li>
                    {{#hvisIkkeTom borSammenMed.verdi}}
                        <span class="verdi verdi--tekst">
                            {{ hentTekst (concat "familie.barn.true.barn.borsammen." borSammenMed.verdi) }}
                        </span>
                    {{else}}
                        <span class="verdi verdi--ikkeUtfylt">
                            <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                        </span>
                    {{/hvisIkkeTom}}
                </li>
                {{#if @root.utvidetSoknad}}
                    {{{hentSvaralternativer "familie.barn.true.barn.borsammen"}}}
                {{/if}}
            </ul>
        </li>
        {{#hvisIkkeTom borSammenMed.verdi}}
            {{#if borSammenMed.verdi}}
                <li>
                    <span class="label">{{ hentTekst "familie.barn.true.barn.deltbosted.sporsmal" }}</span>
                    {{#if @root.utvidetSoknad}}
                        {{{settInnHjelpetekst "familie.barn.true.barn.deltbosted.hjelpetekst.tekst"}}}
                    {{/if}}
                    <ul class="svar-liste">
                        <li>
                            {{#hvisIkkeTom harDeltBosted.verdi}}
                                <span class="verdi verdi--tekst">
                                    {{ hentTekst (concat 'familie.barn.true.barn.deltbosted.' harDeltBosted.verdi) }}
                                </span>
                            {{else}}
                                <span class="verdi verdi--ikkeUtfylt">
                                    <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                                </span>
                            {{/hvisIkkeTom}}
                        </li>
                        {{#if @root.utvidetSoknad}}
                            {{{hentSvaralternativer "familie.barn.true.barn.deltbosted"}}}
                        {{/if}}
                    </ul>
                </li>
            {{else}}
                <li>
                    <span class="label">{{ hentTekst "familie.barn.true.barn.grad.sporsmal" }}</span>
                    <ul class="svar-liste">
                        <li>
                            {{#hvisIkkeTom samvarsgrad.verdi}}
                                <span class="verdi verdi--tekst">
                                    {{ samvarsgrad.verdi }} {{ hentTekst "prosent" }}
                                </span>
                            {{else}}
                                <span class="verdi verdi--ikkeUtfylt">
                                    <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                                </span>
                            {{/hvisIkkeTom}}
                        </li>
                        {{#if @root.utvidetSoknad}}
                            {{{hentSvaralternativer "familie.barn.true.barn.grad"}}}
                        {{/if}}
                    </ul>
                </li>
            {{/if}}
        {{/hvisIkkeTom}}
        {{#if @root.harBrukerregistrerteBarn}}
            {{#if @root.utvidetSoknad}}
                {{{settInnKnappTilgjengeligTekst "familie.barn.true.barn.slett"}}}
            {{/if}}
        {{/if}}
    {{/hvisLik}}
{{/each}}