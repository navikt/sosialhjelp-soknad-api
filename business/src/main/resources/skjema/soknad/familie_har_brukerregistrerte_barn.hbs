{{#if @root.utvidetSoknad}}
    {{{hentTekstMedParametere "familierelasjon.ingress" "antallBarn" forsorgerplikt.ansvar.size}}}
{{/if}}
{{#each forsorgerplikt.ansvar}}
    {{#hvisLik barn.kilde "bruker"}}
        <h3>{{ hentTekst "familie.barn.true.listetittel" }}</h3>
        {{#with barn}}
            <li>
                <span class="label">{{ hentTekst "kontakt.system.personalia.navn" }}</span>
                <ul class="svar-liste">
                    <li>
                        <span class="verdi verdi--tekst">
                            {{navn.fornavn}} {{navn.mellomnavn}} {{navn.etternavn}}
                        </span>
                    </li>
                </ul>
            </li>
            {{#if fodselsdato}}
                <li>
                    <span class="label">{{ hentTekst "familierelasjon.fodselsdato" }}</span>
                    <ul class="svar-liste">
                        <li>
                            {{#hvisIkkeTom fodselsdato}}
                                <span class="verdi verdi--tekst">
                                    {{fodselsdato}}
                                </span>
                            {{else}}
                                <span class="verdi verdi--ikkeUtfylt">
                                    <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                                </span>
                            {{/hvisIkkeTom}}
                        </li>
                    </ul>
                </li>
            {{/if}}
        {{/with}}
        <li>
            <span class="label">{{ hentTekst "familie.barn.true.barn.borsammen.sporsmal" }}</span>
            <ul class="svar-liste">
                <li>
                    {{#hvisIkkeTom borSammenMed.verdi}}
                        <span class="verdi verdi--radio">
                            {{ hentTekst (concat "familie.barn.true.barn.borsammen." verdi) }}
                        </span>
                    {{else}}
                        <span class="verdi verdi--ikkeUtfylt">
                            <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                        </span>
                    {{/hvisIkkeTom}}
                </li>
                {{#if @root.utvidetSoknad}}
                    {{{hentSvaralternativer "familie.barn.true.barn.borsammen"}}}
                {{/if}}
            </ul>
        </li>
        {{#if borSammenMed.verdi}}
            <li>
                <span class="label">{{ hentTekst "familie.barn.true.barn.grad.sporsmal" }}</span>
                <ul class="svar-liste">
                    <li>
                        {{#hvisIkkeTom samvarsgrad.verdi}}
                            <span class="verdi verdi--tekst">
                                {{ samvarsgrad.verdi }} {{ hentTekst "prosent" }}
                            </span>
                        {{else}}
                            <span class="verdi verdi--ikkeUtfylt">
                                <i>{{ hentTekst 'oppsummering.ikkeutfylt' }}</i>
                            </span>
                        {{/hvisIkkeTom}}
                    </li>
                    {{#if @root.utvidetSoknad}}
                        {{{hentSvaralternativer "familie.barn.true.barn.grad"}}}
                    {{/if}}
                </ul>
            </li>
        {{/if}}
    {{/hvisLik}}
{{/each}}